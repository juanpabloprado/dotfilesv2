  name = Juan Prado
[pager]
  branch = false
[alias]
  st = status
  co = checkout
  unstage = reset HEAD --
  # Delete any merged branches, ignoring the active branch.
  tidy = !git branch --merged | grep -E -v '(master|staging)' | grep -E -v '^\\*' | xargs git branch -d
[pull]
  rebase = false
